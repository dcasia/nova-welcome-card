{
    "name": "digital-creative/nova-welcome-card",
    "description": "A configurable version of the `Help card` that comes with Nova.",
    "keywords": [
        "laravel",
        "nova",
        "card"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\NovaWelcomeCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WelcomeCardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
